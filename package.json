{
  "name": "event-soft-api",
  "version": "1.0.0",
  "description": "API del proyecto event soft",
  "main": "lib/index.js",
  "engines": {
    "node": "10.x"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "scripts": {
    "lint": "npx tslint --project tsconfig.json",
    "build": "npx tsc",
    "test": "npx jest",
    "start": "npm run lint && npm run build && npx node lib/index.js"
  },
  "keywords": [
    "API",
    "EventSoft"
  ],
  "author": "Solution Labs",
  "license": "ISC",
  "devDependencies": {
    "tslint": "^5.12.0",
    "typescript": "^3.8.0"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.21",
    "@types/multer": "^1.4.3",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.0.0",
    "@types/validator": "^13.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "inversify": "^5.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "jest": "^26.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.9.15",
    "multer": "^1.4.2",
    "nodemon": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.4",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "tslint": "^5.12.0",
    "typescript": "^3.8.0",
    "uuid": "^8.1.0",
    "validator": "^13.0.0"
  }
}
